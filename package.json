{
  "name": "gamedin-l3-gaming-blockchain",
  "version": "1.0.0",
  "description": "Complete GameDin Layer 3 Gaming Blockchain with NovaSanctum AI Integration",
  "main": "src/index.ts",
  "scripts": {
    "prepare": "husky install",
    "build": "npx hardhat compile && tsc",
    "test": "npx hardhat test",
    "test:watch": "jest --watch",
    "test:coverage": "npx hardhat coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx,html,css,scss,yaml,yml}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,mdx,html,css,scss,yaml,yml}\"",
    "typecheck": "tsc --noEmit",
    "deploy:local": "npx hardhat deploy --network localhost",
    "deploy:testnet": "npx hardhat deploy --network testnet",
    "deploy:mainnet": "npx hardhat deploy --network mainnet",
    "start:node": "npx hardhat node",
    "start:bridge": "node src/bridge/relayer.js",
    "start:gaming": "node src/gaming/real-time-engine.js",
    "start:ai": "node src/ai/nova-sanctum.js",
    "health:check": "node src/monitoring/health-checker.js",
    "dev": "ts-node src/cli/gdi-cli.ts",
    "gdi:balance": "ts-node src/cli/gdi-cli.ts balance",
    "gdi:send": "ts-node src/cli/gdi-cli.ts send",
    "gdi:info": "ts-node src/cli/gdi-cli.ts info",
    "gdi:phantom": "ts-node src/cli/gdi-cli.ts phantom",
    "gdi:history": "ts-node src/cli/gdi-cli.ts history",
    "gdi:check": "ts-node src/cli/gdi-cli.ts check",
    "gdi:help": "ts-node src/cli/gdi-cli.ts help",
    "deploy:complete": "node scripts/deploy-gdi-complete.js",
    "deploy:metadata": "node scripts/deploy-metadata.js",
    "deploy:pool": "node scripts/create-raydium-pool.js",
    "revoke": "node scripts/revoke-authorities.js",
    "status": "node scripts/deploy-gdi-complete.js --status",
    "check-authorities": "node scripts/revoke-authorities.js --check",
    "verify": "node scripts/verify-setup.js",
    "l3:deploy": "./GameDin_Complete_Launch.sh",
    "l3:infrastructure": "./GameDin_Infrastructure_Setup.sh",
    "l3:instant": "./GameDin_Instant_Deploy.sh",
    "l3:quick": "./GameDin_L3_Quick_Start.sh",
    "postinstall": "husky install"
  },
  "keywords": [
    "blockchain",
    "gaming",
    "layer3",
    "ai",
    "novasanctum",
    "ethereum",
    "base",
    "defi",
    "gdi",
    "gamedin",
    "websocket",
    "realtime"
  ],
  "author": "GameDin Team",
  "license": "MIT",
