{
  "name": "gdi-token-deployment",
  "version": "1.0.0",
  "description": "Complete deployment infrastructure for GameDin (GDI) token on Solana with Raydium liquidity pool",
  "main": "scripts/deploy-gdi-complete.js",
  "scripts": {
    "deploy": "node scripts/deploy-gdi-complete.js",
    "deploy:metadata": "node scripts/deploy-metadata.js",
    "deploy:pool": "node scripts/create-raydium-pool.js",
    "revoke": "node scripts/revoke-authorities.js",
    "status": "node scripts/deploy-gdi-complete.js --status",
    "check-authorities": "node scripts/revoke-authorities.js --check",
    "verify": "node scripts/verify-setup.js",
    "build": "tsc",
    "dev": "ts-node src/cli/gdi-cli.ts",
    "gdi:balance": "ts-node src/cli/gdi-cli.ts balance",
    "gdi:send": "ts-node src/cli/gdi-cli.ts send",
    "gdi:info": "ts-node src/cli/gdi-cli.ts info",
    "gdi:phantom": "ts-node src/cli/gdi-cli.ts phantom",
    "gdi:history": "ts-node src/cli/gdi-cli.ts history",
    "gdi:check": "ts-node src/cli/gdi-cli.ts check",
    "gdi:help": "ts-node src/cli/gdi-cli.ts help",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "solana",
    "token",
    "metaplex",
    "raydium",
    "defi",
    "gaming",
    "gdi",
    "gamedin"
  ],
  "author": "GameDin Team",
  "license": "MIT",
  "dependencies": {
    "@metaplex-foundation/js": "^0.20.1",
    "@raydium-io/raydium-sdk": "^1.3.1-beta.0",
    "@solana/spl-token": "^0.3.11",
    "@solana/wallet-adapter-phantom": "^0.9.28",
    "@solana/wallet-adapter-react": "^0.15.39",
    "@solana/wallet-adapter-react-ui": "^0.9.39",
    "@solana/wallet-adapter-wallets": "^0.19.37",
    "@solana/web3.js": "^1.98.2",
    "autoprefixer": "^10.4.21",
    "bs58": "^5.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "dotenv": "^16.3.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gamedin/gdi-token-deployment.git"
  },
  "bugs": {
    "url": "https://github.com/gamedin/gdi-token-deployment/issues"
  },
  "homepage": "https://gamedin.io"
}
