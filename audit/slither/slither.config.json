{
  "filter_paths": [
    "node_modules",
    "test",
    "script",
    "mocks",
    "hardhat.config.ts"
  ],
  "exclude_informational": false,
  "exclude_optimization": false,
  "exclude_low": false,
  "exclude_medium": false,
  "exclude_high": false,
  "solc_remaps": [
    "@openzeppelin/contracts/=node_modules/@openzeppelin/contracts/",
    "@openzeppelin/contracts-upgradeable/=node_modules/@openzeppelin/contracts-upgradeable/",
    "@chainlink/contracts/=node_modules/@chainlink/contracts/",
    "@uniswap/=node_modules/@uniswap/"
  ],
  "solc_args": "--evm-version istanbul",
  "triage_mode": false,
  "checklist": true,
  "detectors_to_exclude": [
    "unused-return",
    "reentrancy-benign",
    "reentrancy-no-eth",
    "assembly"
  ],
  "detectors_to_run": [
    "reentrancy-eth",
    "reentrancy-events",
    "timestamp",
    "assembly",
    "bools",
    "calls-loop",
    "constable-states",
    "constant-function",
    "deprecated-standards",
    "erc20-interface",
    "erc20-indexed",
    "erc721-interface",
    "erc1155-interface",
    "events-access",
    "events-maths",
    "function-init-state",
    "incorrect-equality",
    "low-level-calls",
    "naming-convention",
    "reentrancy-no-eth",
    "reentrancy-benign",
    "reentrancy-events",
    "reentrancy-unlimited-gas",
    "rtlo",
    "shadowing-state",
    "suicidal",
    "unchecked-lowlevel",
    "unchecked-send",
    "uninitialized-state",
    "uninitialized-storage",
    "unused-return",
    "unused-state",
    "unused-return",
    "unused-return",
    "unused-return"
  ],
  "exclude_dependencies": true,
  "json": "slither-report.json",
  "markdown_root": "audit/reports/slither",
  "solc_working_path": ".",
  "solc_solcs_bin": "solc",
  "solc_remaps_enabled": true,
  "solc_force_legacy_json": false,
  "solc_allow_path": ".",
  "solc_standard_json_input": false,
  "solc_disable_warnings": false,
  "solc_optimize": true,
  "solc_optimize_runs": 200,
  "solc_optimize_yul": true,
  "solc_via_ir": false,
  "solc_evm_version": "istanbul",
  "solc_remap_imports": {},
  "solc_remap_imports_file": "",
  "solc_remap_imports_allow_paths": [],
  "solc_remap_imports_allow_paths_file": "",
  "solc_remap_imports_allow_paths_recursive": false,
  "solc_remap_imports_allow_paths_recursive_file": "",
  "solc_remap_imports_allow_paths_recursive_depth": 10,
  "solc_remap_imports_allow_paths_recursive_depth_file": "",
  "solc_remap_imports_allow_paths_recursive_depth_max": 20
}
